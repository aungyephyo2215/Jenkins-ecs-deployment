{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "Jenkins-learn-app",
    "containerDefinitions": [
        {
            "name": "jenkins-learnapp",
            "image": "mainline-alpine",
            "portMappings": [{
                "name": "nginx-app",
                "containerPort": 80,
                "hostPort": 80,
                "protocol": "tcp",
                "appProtocol": "http"
            }],
            "essential": true
            
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256 ",
    "executionRoleArn": "arn:aws:iam::661269962563:role/ecsTaskExecutionRole"
}